{
    "name": "yuloh/json-schema-benchmark",
    "require": {
        "justinrainbow/json-schema": "^2.0",
        "league/json-guard": "0.2.0@dev",
        "json-schema/JSON-Schema-Test-Suite": "1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Yuloh\\JsonSchemaBenchmark\\": "src"
        }
    },
    "authors": [
        {
            "name": "Matt Allan",
            "email": "matthew.james.allan@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "json-schema/JSON-Schema-Test-Suite",
                "version": "1.1.0",
                "source": {
                    "url": "https://github.com/json-schema/JSON-Schema-Test-Suite",
                    "type": "git",
                    "reference": "1.1.0"
                }
            }
        }
    ],
    "scripts": {
        "bench-meta": "phpbench run ./src/MetaValidationBench.php --report=aggregate --output=markdown --progress=none > report-meta.md",
        "bench-composer": "phpbench run ./src/ComposerBench.php --report=aggregate --output=markdown --progress=none > report-composer.md"
    }
}
